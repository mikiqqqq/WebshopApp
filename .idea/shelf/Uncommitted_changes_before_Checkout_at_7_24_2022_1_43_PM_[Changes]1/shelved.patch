@@ -0,0 +1,21 @@
+import axios from 'axios';
+
+const EMPLOYEE_API_BASE_URL = "http://localhost:8080/api/items";
+
+class ItemService {
+
+    fetchAllItems(){
+        return axios.get(EMPLOYEE_API_BASE_URL + '/fetch-all');
+    }
+
+    findByName(itemName:string){
+        return axios.get(EMPLOYEE_API_BASE_URL + '/by-name/' + itemName);
+    }
+
+    findAllThatContainTarget(target:string){
+        return axios.get(EMPLOYEE_API_BASE_URL + '/contain/' + target);
+
+    }
+}
+
+export default new ItemService()
